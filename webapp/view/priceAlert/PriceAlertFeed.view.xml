<!DOCTYPE xml>
<mvc:View
	controllerName="trading-cockpit-frontend.controller.priceAlert.PriceAlertFeed"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:u="sap.ui.unified"
	xmlns:core="sap.ui.core">
	
	<Page title="{i18n>priceAlertFeed.header}">
		<content>
			<List items="{path: 'priceAlerts>/priceAlert/'}" >
				<FeedListItem text="{parts: [{path: 'priceAlerts>symbol'}, {path: 'priceAlerts>stockExchange'}], formatter: '.priceAlertTextFormatter'}">
				</FeedListItem>
			</List>
		</content>
	</Page>
</mvc:View>